

public class Tic_Tac_Toe
{
	public static char Create_Board()
	{
		int rows = 3;
		int colums = 3;
		char[][]array = new char[rows][colums];
		for(int i = 0; i < rows; i++)
		{
			for(int j = 0; j < colums; j++)
			{
				array[i][j] = '*';
			}
		}
	
	return array;
	}
	public static String Write_Board()
	{	
		String Board;
		
		Board  = "   1 2 3 \n";
		Board += "---------\n";
		Board += "1||(("1||") + array[0][0] + ("|") + array[0][1] + ("|") + array[0][2] + ("|"));
(("2||") + array[1][0] + ("|") + array[1][1] + ("|") + array[1][2] + ("|"));
(("3||") + array[2][0] + ("|") + array[2][1] + ("|") + array[2][2] + ("|"));
\n";
		Board += "2||*|*|*|\n";
		Board += "3||*|*|*|\n";
			
		return Board;
		
